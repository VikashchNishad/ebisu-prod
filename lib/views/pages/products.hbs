    };

    $scope.loadingHr = false;

    $scope.gotoElement = function (eID){
      // set the location.hash to the id of
      // the element you wish to scroll to.
      $location.hash('bottom');
      // call $anchorScroll()
      anchorSmoothScroll.scrollTo(eID);
    };

    $scope.gotoMobileElement = function (eID){
      // set the location.hash to the id of
      // the element you wish to scroll to.
      $location.hash('mobileBottom');
      // call $anchorScroll()
      anchorSmoothScroll.scrollTo(eID);
    };

     // $scope.domain = "http://139.59.37.234:80/api/";
    // :3000 
    $scope.domain = "http://"+ window.location.host + "/api/";
    //$scope.domain = "http://kadence.in/api/";

    $scope.sizem = "fadeInRight";
    $scope.sizeimg = "fadeInRight";

    $http.get($scope.domain +"cloudinaryname/cloudinaryName")
      .then(function(response) {
          $scope.cloudinaryname = response.data;
          // console.log($scope.cloudinaryname.cloudinaryCloudName);
    });

    $http.get($scope.domain +"productscategory/")
      .then(function(response) {
        $scope.menu = response.data;
    });

    $http.get($scope.domain +"filtersubcategory/productscategorymy/:Guitars Basses")
      .then(function(response) {
        $scope.guitarsproducts = response.data.dataList;
    });
    
    $http.get($scope.domain +"filtersubcategory/productscategorymy/:Ukulele")
      .then(function(response) {
        $scope.ukuleleproducts = response.data.dataList;
    });    
    
    $http.get($scope.domain +"filtersubcategory/productscategorymy/:Drums percussion")
      .then(function(response) {
        $scope.drumsproducts = response.data.dataList;
    });

    $http.get($scope.domain +"filtersubcategory/productscategorymy/:Digital")
      .then(function(response) {
        $scope.digitalproducts = response.data.dataList;
    });

    $http.get($scope.domain +"filtersubcategory/productscategorymy/:accessories")
      .then(function(response) {
        $scope.accessoriesproducts = response.data.dataList;
    });

    $http.get($scope.domain +"filtersubcategory/productscategorymy/:Wind Instruments")
      .then(function(response) {
        $scope.windInstruments = response.data.dataList;
    });

    $http.get($scope.domain +"filtersubcategory/productscategorymy/:Amplifiers ")
      .then(function(response) {
        $scope.amplifiers  = response.data.dataList;
    });
    
    $scope.showif = true;
